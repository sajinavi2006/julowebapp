  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "env-cmd -f .env.local craco start",
    "start:dev": "env-cmd -f .env.development craco start",
    "start:staging": "env-cmd -f .env.staging craco start",
    "start:uat": "env-cmd -f .env.uat craco start",
    "start:prod": "env-cmd -f .env.production craco start",
    "build": "env-cmd -f .env.local craco build",
    "build:dev": "env-cmd -f .env.development craco build",
    "build:staging": "env-cmd -f .env.staging craco build",
    "build:uat": "env-cmd -f .env.uat craco build",
    "build:prod": "env-cmd -f .env.production craco build",
    "test": "env-cmd -f .env.local craco test",
    "test:dev": "env-cmd -f .env.development craco test",
    "test:staging": "env-cmd -f .env.staging craco test",
    "test:uat": "env-cmd -f .env.uat craco test",
    "test:prod": "env-cmd -f .env.production craco test",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix",
    "ts-coverage": "node ./scripts/count-ts-file.ts",
    "ts-coverage:report": "typescript-coverage-report -s -i \"**/*.js\" && npm run ts-coverage",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@julofinance/eslint-config": "0.0.8",
    "@julofinance/tsconfig": "0.0.5",
