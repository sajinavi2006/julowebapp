    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "octokit": "^2.0.4",
    "prettier": "^2.3.0",
    "react-app-env": "^1.2.3",
    "typescript-coverage-report": "^0.6.4",
    "url-loader": "^4.1.1"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "typeCoverage": {
    "atLeast": 10
  },
  "resolutions": {
    "@types/react": "17.0.30"
  },
  "jest": {
    "testMatch": ["<rootDir>/src/**/__tests__/**/*.{test,spec}.{js,jsx,ts,tsx}"]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": ["eslint", "prettier --write"]
  }
}
